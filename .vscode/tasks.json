{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "generate",
      "problemMatcher": ["$eslint-compact"]
    },
    {
      "type": "npm",
      "script": "build:re",
      "problemMatcher": ["$ocamlc"]
    },
    {
      "type": "npm",
      "script": "build:re:watch",
      "problemMatcher": ["$ocamlc"]
    },
    {
      "type": "npm",
      "script": "build:ts:watch",
      "problemMatcher": ["$eslint-compact"]
    }
  ]
}
